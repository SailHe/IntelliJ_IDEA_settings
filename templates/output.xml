<templateSet group="output">
  <template name="nsout" value="System.out.println($Num$ + &quot;$String$&quot;);$END$" description="数字加字串换行输出" toReformat="true" toShortenFQNames="true">
    <variable name="Num" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="String" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="souf Int" value="System.out.printf(&quot;$a$%d$b$\r\n&quot;, $c$);$END$" description="打印一个格式化整型数字并回车换行" toReformat="true" toShortenFQNames="true">
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="b" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="c" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="COMPLETION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
    </context>
  </template>
  <template name="souf Double" value="System.out.printf(&quot;$o$%.2$a$f$b$\r\n&quot;, $c$);$END$" description="打印一个格式化浮点数字并回车换行" toReformat="false" toShortenFQNames="true">
    <variable name="o" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="a" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="b" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="c" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="COMPLETION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
    </context>
  </template>
  <template name="newInput" value="static Scanner in = new Scanner(System.in);" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>